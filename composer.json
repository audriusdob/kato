{
    "name": "perminder-klair/kato",
    "description": "Kato CMS based on Yii 2",
    "keywords": ["yii", "framework", "kato", "kato application"],
    "homepage": "https://github.com/perminder-klair/kato",
    "type": "project",
    "license": "The MIT License",
    "version": "0.9.4",
    "authors": [
        {
            "name": "Parminder Klair",
            "email": "perminder.klair@gmail.com",
            "homepage": "http://www.webonise.co.uk/"
        }
    ],
    "support": {
        "issues": "https://github.com/perminder-klair/kato/issues?state=open",
        "wiki": "https://github.com/perminder-klair/kato/wiki",
        "source": "https://github.com/perminder-klair/kato"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-imagine": "*",
        "perminder-klair/kato-core": "dev-master",
        "perminder-klair/yii2-sir-trevor-js": "dev-master",
        "michelf/php-smartypants": "1.6.0-beta1",
        "kartik-v/yii2-widgets": "dev-master",
        "kartik-v/yii2-markdown": "dev-master",
        "asofter/yii2-imperavi-redactor": "*",
        "2amigos/yii2-editable-widget" : "*"

    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
