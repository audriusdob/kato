{
    "name": "perminder-klair/kato",
    "description": "Kato CMS based on Yii 2",
    "keywords": ["yii", "framework", "kato", "kato application"],
    "homepage": "https://github.com/perminder-klair/kato",
    "type": "project",
    "license": "The MIT License",
    "version": "0.1.0",
    "authors": [
        {
            "name": "Parminder Klair",
            "email": "perminder.klair@gmail.com",
            "homepage": "http://www.webonise.co.uk/"
        }
    ],
    "support": {
        "issues": "https://github.com/perminder-klair/kato/issues?state=open",
        "wiki": "https://github.com/perminder-klair/kato/wiki",
        "source": "https://github.com/perminder-klair/kato"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "dev-master",
        "yiisoft/yii2-swiftmailer": "dev-master",
        "yiisoft/yii2-bootstrap": "dev-master",
        "yiisoft/yii2-imagine": "*",
        "perminder-klair/kato-core": "dev-master",
        "perminder-klair/yii2-dropzone": "dev-master",
        "perminder-klair/yii2-sir-trevor-js": "dev-master",
        "kartik-v/yii2-markdown": "dev-master",
        "kartik-v/yii2-widgets": "dev-master",
        "asofter/yii2-imperavi-redactor": "*"

    },
    "require-dev": {
        "yiisoft/yii2-codeception": "dev-master",
        "yiisoft/yii2-debug": "dev-master",
        "yiisoft/yii2-gii": "dev-master"
    },
    "suggest": {
        "codeception/codeception": "Codeception, 1.8.*@dev is currently works well with Yii.",
        "codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
        "codeception/verify": "BDD Assertions for PHPUnit and Codeception",
        "yiisoft/yii2-faker": "Fixtures generator for Yii2 based on Faker lib"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::setPermission"
        ]
    },
    "extra": {
        "writable": [
            "backend/runtime",
            "backend/web/assets",

            "console/runtime",
            "console/migrations",

            "frontend/runtime",
            "frontend/web/assets"
        ]
    }
}
